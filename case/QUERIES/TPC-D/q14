(PROJECT(.PROMO_REV),
	(FUNC_OP(<.PROMO, .SUM> AS PROMO_REV),
		(AGG_LIST((<L.LE, L.LD, P.PT> AS PROMO, <L.LE, L.LD> AS SUM), (GROUP_BY <>)),
			(EQJOIN(L.LPK, P.PPK),
				(SELECT,
					GET("L"),
					(OP_AND,
						(OP_GE,
							ATTR("L.LSD"),
							INT("1000")
						),
						(OP_LT,
							ATTR("L.LSD"),
							INT("1030")
						)
					)
				),
				GET("P")
			)
		)
	)
)